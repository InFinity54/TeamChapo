<div class="tab-pane fade overflow-y-hidden" id="revenus" role="tabpanel" aria-labelledby="revenus-tab">
    <table class="table table-striped w-100">
        <thead>
            <th></th>
            {% for i in 0..4 %}
                {% if game.teams.100.players[i] is not null %}
                    <th class="text-center p-0"><div class="blueTeamRevenus"></div><span class="teamSummonerRevenus">{% if game.teams.100.players[i].lane is not null %}<img src="{{ asset("riot/lol/img/lanes/" ~ game.teams.100.players[i].lane.image) }}" alt="{{ game.teams.100.players[i].lane.name }}" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ game.teams.100.players[i].lane.name }}" />{% else %}<img src="{{ asset("riot/lol/img/lanes/unknown.png") }}" alt="Inconnu" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="Poste inconnu" />{% endif %}<br /><img src="{{ asset("riot/lol/img/champion/tiles/" ~ game.teams.100.players[i].champion.image) }}" alt="{{ game.teams.100.players[i].champion.name }}" width="60px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ game.teams.100.players[i].champion.name }}" /><br/>{{ game.teams.100.players[i].summonerName }}</span></th>
                {% endif %}
            {% endfor %}
            {% if game.teams.100.players[5]|length > 0 %}
                {% for player in game.teams.100.players[5] %}
                    {% if player is not null %}
                        <th class="text-center p-0"><div class="blueTeamRevenus"></div><span class="teamSummonerRevenus">{% if player.lane is not null %}<img src="{{ asset("riot/lol/img/lanes/" ~ player.lane.image) }}" alt="{{ player.lane.name }}" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ player.lane.name }}" />{% else %}<img src="{{ asset("riot/lol/img/lanes/unknown.png") }}" alt="Inconnu" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="Poste inconnu" />{% endif %}<br /><img src="{{ asset("riot/lol/img/champion/tiles/" ~ player.champion.image) }}" alt="{{ player.champion.name }}" width="60px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ player.champion.name }}" /><br />{{ player.summonerName }}</span></th>
                    {% endif %}
                {% endfor %}
            {% endif %}
            {% for i in 0..4 %}
                {% if game.teams.200.players[i] is not null %}
                    <th class="text-center p-0"><div class="redTeamRevenus"></div><span class="teamSummonerRevenus">{% if game.teams.200.players[i].lane is not null %}<img src="{{ asset("riot/lol/img/lanes/" ~ game.teams.200.players[i].lane.image) }}" alt="{{ game.teams.200.players[i].lane.name }}" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ game.teams.200.players[i].lane.name }}" />{% else %}<img src="{{ asset("riot/lol/img/lanes/unknown.png") }}" alt="Inconnu" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="Poste inconnu" />{% endif %}<br /><img src="{{ asset("riot/lol/img/champion/tiles/" ~ game.teams.200.players[i].champion.image) }}" alt="{{ game.teams.200.players[i].champion.name }}" width="60px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ game.teams.200.players[i].champion.name }}" /><br />{{ game.teams.200.players[i].summonerName }}</span></th>
                {% endif %}
            {% endfor %}
            {% if game.teams.200.players[5]|length > 0 %}
                {% for player in game.teams.200.players[5] %}
                    {% if player is not null %}
                        <th class="text-center p-0"><div class="redTeamRevenus"></div><span class="teamSummonerRevenus">{% if player.lane is not null %}<img src="{{ asset("riot/lol/img/lanes/" ~ player.lane.image) }}" alt="{{ player.lane.name }}" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ player.lane.name }}" />{% else %}<img src="{{ asset("riot/lol/img/lanes/unknown.png") }}" alt="Inconnu" width="20px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="Poste inconnu" />{% endif %}<br /><img src="{{ asset("riot/lol/img/champion/tiles/" ~ player.champion.image) }}" alt="{{ player.champion.name }}" width="60px" height="auto" data-bs-toggle="tooltip" data-bs-placement="top" title="{{ player.champion.name }}" /><br />{{ player.summonerName }}</span></th>
                    {% endif %}
                {% endfor %}
            {% endif %}
        </thead>
        <tbody>
            {% include "site/pages/team/games/parts/subparts/revenus/detail.revenus.total.html.twig" %}
            {% include "site/pages/team/games/parts/subparts/revenus/detail.revenus.used.html.twig" %}
            {% include "site/pages/team/games/parts/subparts/revenus/detail.revenus.creeps.html.twig" %}
            {% include "site/pages/team/games/parts/subparts/revenus/detail.revenus.monsters.html.twig" %}
